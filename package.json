{
  "name": "@phoeon/free-ui",
  "version": "0.1.0beta.3",
  "main": "./src/index.ts",
  "scripts": {
    "serve": "vite",
    "build": "npm run build:doc && npm run build:demo",
    "build:www": "vue-cli-service build --mode www",
    "build:doc": "vue-cli-service build --mode doc",
    "build:demo": "vue-cli-service build --mode demo",
    "lint": "vue-cli-service lint",
    "dev": "vite",
    "build1": "yarn gen-version && yarn appmeta && vue-tsc --noEmit && vite build",
    "build:site": "vue-tsc --noEmit && vite build --mode site",
    "package": "yarn clean && vue-tsc --noEmit && vite build -c vite.package.config.ts && rm dist/*.ico",
    "clean": "rimraf dist",
    "serve1": "vite preview",
    "gen-version": "node build/gen-version.js",
    "appmeta": "node build/gen-app-meta.js",
    "router": "node build/inspect.js"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/phoeon/free-ui.git"
  },
  "keywords": [
    "vue3",
    "free",
    "vue",
    "components",
    "ui",
    "typescript"
  ],
  "files": [
    "src/assets/style",
    "src/components",
    "src/directives",
    "src/shared",
    "src/index.ts",
    "dist"
  ],
  "dependencies": {
    "ph-evt": "^1.0.0",
    "ph-gesture": "*",
    "ph-position": "*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "highlight.js": "^11.3.1",
    "marked": "^4.0.5",
    "ph-copy": "^1.0.0",
    "ph-evt": "^1.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue-tsc": "^0.3.0"
  }
}
