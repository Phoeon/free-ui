{
    "compilerOptions": {
      "paths": {
        "@phoeon/free-ui":["./src"]
      },
      "strict": true,
      "strictNullChecks": true,
      "module": "esnext",
      "moduleResolution": "Node",
      "declaration": true,
      "rootDir": "./src",
      "jsx": "preserve",
      "target": "esnext",
      "esModuleInterop": true,
      "lib": ["esnext", "dom"]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
  }